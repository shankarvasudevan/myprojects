@page "/todos"

<div>
    <h2>Todos</h2>
    @foreach (var todo in todos)
    {
        <div>@todo.Title</div>
    }
</div>

@code {
    public class Todo 
    {
        public string Title { get; set; }
    }

    List<Todo> todos = new List<Todo>() 
    { 
        new Todo {Title="clean"}, 
        new Todo {Title="shop"}
    };
}